{
  "name": "zhyu/vue.crud",
  "description": "vue crud & laravel admin",
  "license": "MIT",
  "keywords": [
    "vue",
    "crud",
    "admin"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "predis/predis": "^1.1",
    "zhyu/packages": "0.4.0.4"
  },
  "autoload": {
    "psr-4": {
      "ZhyuVueCurd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Zhyu\\Tests\\VueCrud\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.1-dev"
    },
    "laravel": {
      "providers": [
        "ZhyuVueCurd\\ZhyuServiceProvider"
      ],
      "aliases": {

      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^8",
    "barryvdh/laravel-ide-helper": "^2.7"
  }
}
