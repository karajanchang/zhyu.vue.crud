{
  "name": "zhyu/vue.crud",
  "description": "vue crud & laravel admin",
  "license": "MIT",
  "keywords": [
    "vue",
    "crud",
    "admin"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "predis/predis": "^1.1",
    "zhyu/packages": "^0.4",
    "livewire/livewire": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "ZhyuVueCurd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZhyuVueCurd\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.1-dev"
    },
    "laravel": {
      "providers": [
        "ZhyuVueCurd\\ZhyuServiceProvider"
      ],
      "aliases": {

      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('resources/views/vendor/livewire/admin') || mkdir('resources/views/vendor/livewire/admin', 0755, true);\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/config-form.blade.php', 'resources/views/vendor/livewire/admin/config-form.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/config-list.blade.php', 'resources/views/vendor/livewire/admin/config-list.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/config-name-value.blade.php', 'resources/views/vendor/livewire/admin/config-name-value.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/config-set-value.blade.php', 'resources/views/vendor/livewire/admin/config-set-value.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/left-menu.blade.php', 'resources/views/vendor/livewire/admin/left-menu.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/menu-breadcrumb.blade.php', 'resources/views/vendor/livewire/admin/menu-breadcrumb.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/table-index.blade.php', 'resources/views/vendor/livewire/admin/table-index.blade.php');\"",
      "@php -r \"copy('vendor/zhyu/vue.crud/src/views/livewire/admin/table-row-drag.blade.php', 'resources/views/vendor/livewire/admin/table-row-drag.blade.php');\""
    ]
  }
}
