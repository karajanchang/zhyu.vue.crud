{
  "name": "zhyu/vue.crud",
  "description": "vue crud & laravel admin",
  "license": "MIT",
  "keywords": [
    "vue",
    "crud",
    "admin"
  ],
  "authors": [
    {
      "name": "karajanchang",
      "email": "karajanchang@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.5",
    "predis/predis": "^1.1",
    "zhyu/packages": "^0.4",
    "livewire/livewire": "^2.3",
    "intervention/image": "^2.5",
    "spatie/laravel-ray": "^1.23"
  },
  "autoload": {
    "psr-4": {
      "ZhyuVueCurd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZhyuVueCurd\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.1-dev"
    },
    "laravel": {
      "providers": [
          "ZhyuVueCurd\\ZhyuServiceProvider",
          "Intervention\\Image\\ImageServiceProvider"
      ],
      "aliases": {

      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-root-package-install": [
    ]
  }
}
